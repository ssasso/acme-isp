{
    "image": "ghcr.io/ipspace/netlab/devcontainer:latest",
    "hostRequirements": {
        "cpus": 2,
        "memory": "8gb",
        "storage": "32gb"
    },
    "forwardPorts": [ 5001 ],
    "portsAttributes": {
        "5001": {
            "label": "Edgeshark",
            "onAutoForward": "notify"
        }
    },
    "postAttachCommand": "./.devcontainer/post-up.sh"
}
